(action order (?b1:Block ?s1:Platform ?b2:Block)
	(constraints (neq ?b1 ?s1) (neq ?b2 ?s1) (neq ?b1 ?b2))
	(preconditions (clear ?b2) (clear ?b1) (on-top ?b1 ?s1))
	(effects (not (clear ?b2)) (clear ?s1) (on-top ?b1 ?b2) (not (on-top ?b1 ?s1)))
	)
	
(action move-to-table (?b1:Block ?b2:Block)
	(constraints (neq ?b1 ?b2))
	(preconditions (clear ?b1) (on-top ?b1 ?b2))
	(effects (clear ?b2) (not (on-top ?b1 ?b2)) (on-top ?b1 table))
	)
